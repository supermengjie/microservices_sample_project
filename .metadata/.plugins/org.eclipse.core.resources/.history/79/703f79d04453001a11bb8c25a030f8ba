package com.infosys.equipment.dto;

public class EquipmentDTO {
	private int Id;
	private String name;
	private int customerID;
	
	public int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}


	
}
